    <owl:Class rdf:about="#Heterotroph">
        <rdfs:subClassOf rdf:resource="linnaeantaxonomy:Species"/>
    </owl:Class>

    <owl:Class rdf:about="#Animal">
        <rdfs:subClassOf rdf:resource="linnaeantaxonomy:Species"/>
